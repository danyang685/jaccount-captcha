{
  "name": "jAcap",
  "description": "recognize and fill captcha on jAccount",
  "version": "1.4",
  "content_scripts": [{
    "matches":["https://jaccount.sjtu.edu.cn/*"],
    "js":["main.js"],
    "run_at":"document_start",
    "all_frame":true
  }],
  "permissions": ["tabs","storage"],
  "browser_action": {
      "default_icon": "ja.png",
      "name": "Click to re-recognize",
      "default_popup":"popup.html"
  },
  "manifest_version": 2,
  "icons": {"48": "ja.png"}
}
